{
  "name": "secure_task_management_system",
  "version": "1.0.0",
  "description": "Secure Task Management System",
  "keywords": [
    "NodeJS",
    "Sequelize",
    "Javascript",
    "Database",
    "Task_Management"
  ],
  "license": "MIT",
  "author": "Mayank Kumar Das",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "nodemon app.js",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:migrate": "sequelize-cli db:migrate",
    "db:seed:all": "sequelize-cli db:seed:all",
